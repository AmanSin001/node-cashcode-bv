{
  "name": "cashcode-bv",
  "version": "0.1.0",
  "description": "Library for working with \"CashCode\" bill validator devices from Node.js app.",
  "author": "Desionlab <fenixphp@gmail.com>",
  "keywords": [
    "nodejs",
    "serialport",
    "promise",
    "hardware",
    "electron",
    "cashcode",
    "ccnet",
    "bill-validator"
  ],
  "main": "build/index.js",
  "types": "build/index.d.ts",
  "scripts": {
    "develop": "./node_modules/.bin/tsc --watch",
    "build": "./node_modules/.bin/tsc && npm run test && npm run docs",
    "docs": "typedoc --theme minimal --target 'esnext' --excludeNotExported --excludePrivate --ignoreCompilerErrors --out ./docs/ ./src/",
    "format": "prettier --write \"src/**/*.ts\"",
    "lint": "tslint -p tsconfig.json -c tslint.json",
    "test": "node ./node_modules/mocha/bin/mocha --require ts-node/register  test/**/*.spec.ts",
    "prepublish": "npm run build"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/desionlab/node-cashcode-bv.git"
  },
  "bugs": {
    "url": "https://github.com/desionlab/node-cashcode-bv/issues"
  },
  "homepage": "https://github.com/desionlab/node-cashcode-bv#readme",
  "dependencies": {
    "serialport": "^7.1.5"
  },
  "devDependencies": {
    "@types/chai": "^4.2.1",
    "@types/mocha": "^5.2.7",
    "@types/node": "^12.7.4",
    "@types/serialport": "^7.0.4",
    "chai": "^4.2.0",
    "prettier": "^1.18.2",
    "mocha": "^6.2.0",
    "ts-node": "^8.3.0",
    "tslint": "^5.19.0",
    "typedoc": "^0.15.0",
    "typescript": "^3.6.2"
  },
  "license": "MIT"
}
